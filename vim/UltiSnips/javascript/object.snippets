global !p

# ~/.vim/pythonx/javascript_helpers.py
from javascript_helpers import (
    maybe_semi
)

endglobal

snippet key-value "Object Key-value pair" i
${1:key}: ${2:$1}${3:,}$0
endsnippet

snippet object "Object data type" i
{ ${1:${2:key}: ${3:value}} }$0
endsnippet

snippet "\b(Object\.|object )?freeze" "Object.freeze(obj)" r
Object.freeze(${1:${VISUAL:obj}})$0
endsnippet

snippet "\b(Object\.|object )keys" "Object.keys(obj)" r
Object.keys(${1:${VISUAL:obj}})$0
endsnippet
